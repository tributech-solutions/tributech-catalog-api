{
  "root": "libs/self-description",
  "sourceRoot": "libs/self-description/src",
  "projectType": "library",
  "prefix": "tt",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/self-description"],
      "options": {
        "project": "libs/self-description/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/self-description/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/self-description/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/test"],
      "options": {
        "jestConfig": "libs/test/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/test/src/**/*.ts", "libs/test/src/**/*.html"]
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "buildTarget": "production"
      }
    }
  },
  "tags": []
}
